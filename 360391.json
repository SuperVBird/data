24320;&#23637;&#19979;&#21435;&#65292;&#23545;&#20110;&#38271;&#26399;&#20105;&#22842;&#20234;&#25289;&#20811;&#25511;&#21046;&#26435;&#30340;&#32654;&#22269;&#21644;&#20234;&#26391;&#32780;&#35328;&#65292;&#25110;&#35768;&#24847;&#21619;&#30528;&#19968;&#20010;&#25103;&#21095;&#24615;&#30340;&#36716;&#21464;&#12290;&#32780;&#19988;&#65292;&#36825;&#36824;&#21487;&#33021;&#20196;&#32654;&#22269;&#21450;&#20854;&#32769;&#23545;&#22836;&#20160;&#21494;&#27966;&#27665;&#20853;&#21462;&#24471;&#21033;&#30410;&#19978;&#30340;&#19968;&#33268;&#12290;&#22312;&#32654;&#20891;&#38271;&#26399;&#21344;&#39046;&#20234;&#25289;&#20811;&#26399;&#38388;&#65292;&#20160;&#21494;&#27966;&#27665;&#20853;&#32452;&#32455;&#26432;&#23475;&#20102;&#24456;&#22810;&#32654;&#22269;&#22763;&#20853;&#12290;</p>\n<p>&#36825;&#27425;&#32654;&#22269;&#25193;&#22823;&#20891;&#20107;&#34892;&#21160;&#65292;&#21453;&#26144;&#20102;&#33258;2011&#24180;&#32654;&#20891;&#25764;&#31163;&#21518;&#65292;&#20234;&#25289;&#20811;&#23616;&#21183;&#24694;&#21270;&#30340;&#20005;&#37325;&#31243;&#24230;&#12290;&#20294;&#26159;&#65292;&#20219;&#20309;&#25903;&#25345;&#20160;&#21494;&#27966;&#27665;&#20853;&#30340;&#20915;&#23450;&#65292;&#26412;&#36523;&#20063;&#20250;&#24102;&#26469;&#19968;&#31995;&#21015;&#25361;&#25112;&#12290;&#20107;&#23454;&#24050;&#32463;&#35777;&#26126;&#65292;&#20160;&#21494;&#27966;&#27665;&#20853;&#32452;&#32455;&#30340;&#25112;&#26007;&#21147;&#65292;&#24378;&#20110;&#32654;&#22269;&#35757;&#32451;&#30340;&#20234;&#25289;&#20811;&#25919;&#24220;&#20891;&#12290;</p>\n<p>&#36817;&#20960;&#20010;&#26376;&#26469;&#65292;ISIS&#27494;&#35013;&#20998;&#23376;&#20043;&#25152;&#20197;&#33021;&#22815;&#22312;&#20234;&#25289;&#20811;&#38271;&#39537;&#30452;&#20837;&#65292;&#37096;&#20998;&#21407;&#22240;&#26159;&#36874;&#23612;&#27966;&#35273;&#24471;&#65292;&#21069;&#24635;&#29702;&#21162;&#37324;&middot;&#21345;&#36808;&#21202;&middot;&#39532;&#21033;&#22522;(Nuri Kamal al-Maliki)&#25191;&#25919;&#26399;&#38388;&#39046;&#23548;&#30340;&#20197;&#20160;&#21494;&#27966;&#20026;&#20027;&#30340;&#25919;&#24220;&#25490;&#25380;&#20102;&#20182;&#20204;&#12290;&#32780;&#19988;&#22312;2006&#21644;2007&#24180;&#30340;&#23447;&#27966;&#25112;&#20105;&#26399;&#38388;&#65292;&#20160;&#21494;&#27966;&#27665;&#20853;&#32452;&#32455;&#20196;&#36874;&#23612;&#27966;&#20154;&#24515;&#24822;&#24822;&#12290;&#22914;&#26524;&#32654;&#22269;&#34987;&#35270;&#20026;&#22312;&#21161;&#38271;&#36825;&#20123;&#27665;&#20853;&#32452;&#32455;&#30340;&#27668;&#28976;&#65292;&#37027;&#20040;&#20154;&#21475;&#22788;&#20110;&#21155;&#21183;&#30340;&#36874;&#23612;&#27966;&#21487;&#33021;&#23601;&#19981;&#20250;&#21442;&#19982;&#32654;&#22269;&#30340;&#38271;&#26399;&#30446;&#26631;&#65306;&#22312;&#20234;&#25289;&#20811;&#32452;&#25104;&#22242;&#32467;&#25919;&#24220;&#12290;</p>\n<p>&#25110;&#32773;&#65292;&#22312;&#26368;&#31967;&#31957;&#30340;&#24773;&#20917;&#19979;&#65292;&#20250;&#26377;&#26356;&#22810;&#36874;&#23612;&#27966;&#20154;&#22763;&#21644;ISIS&#27494;&#35013;&#20998;&#23376;&#32467;&#30431;&#12290;</p>\n<p>&#20960;&#20010;&#26376;&#21069;&#65292;&#24403;&#22885;&#24052;&#39532;&#25919;&#24220;&#21463;&#21040;&#20234;&#25289;&#20811;&#31532;&#20108;&#22823;&#22478;&#24066;&#25705;&#33487;&#23572;&#22833;&#23432;&#30340;&#21050;&#28608;&#65292;&#22312;&#26435;&#34913;&#25239;&#20987;ISIS&#30340;&#20891;&#20107;&#34892;&#21160;&#26102;&#65292;&#25140;&#32500;&middot;&#24444;&#24471;&#38647;&#20044;&#26031;(David Petraeus)&#23601;&#26366;&#23545;&#36825;&#31181;&#21487;&#33021;&#24615;&#21457;&#20986;&#35686;&#21578;&#12290;&#27492;&#20154;&#26366;&#26159;&#39547;&#20234;&#25289;&#20811;&#30340;&#32654;&#20891;&#26368;&#39640;&#25351;&#25381;&#23448;&#65292;&#39046;&#23548;&#36807;2007&#24180;&#30340;&#22686;&#20853;&#34892;&#21160;&#12290;</p>\n<p>&ldquo;&#36825;&#19981;&#21487;&#20197;&#26159;&#32654;&#22269;&#20026;&#20160;&#21494;&#27966;&#27665;&#20853;&#65292;&#25110;&#32773;&#20026;&#38463;&#25289;&#20271;&#19990;&#30028;&lsquo;&#20160;&#21494;&#27966;&#23545;&#36874;&#23612;&#27966;&rsquo;&#30340;&#26007;&#20105;&#25552;&#20379;&#31354;&#20013;&#25903;&#25588;&#65292;&rdquo;&#20170;&#24180;6&#26376;&#65292;&#20182;&#22312;&#20262;&#25958;&#30340;&#19968;&#27425;&#23433;&#20840;&#20250;&#35758;&#19978;&#35828;&#12290;&ldquo;&#36825;&#24517;&#39035;&#26159;&#20840;&#20307;&#20234;&#25289;&#20811;&#20154;&#25171;&#20987;&#26497;&#31471;&#20998;&#23376;&#30340;&#26007;&#20105;&#65292;&#21482;&#19981;&#36807;&#36825;&#20123;&#26497;&#31471;&#20998;&#23376;&#24688;&#22909;&#26159;&#36874;&#23612;&#27966;&#38463;&#25289;&#20271;&#20154;&#12290;&rdquo;</p>\n<p>&#21608;&#26085;&#30340;&#26102;&#20505;&#65292;&#32654;&#22269;&#35880;&#24910;&#22320;&#34920;&#31034;&#65292;&#33258;&#24049;&#27491;&#19982;&#30431;&#21451;&#22312;&#38463;&#33707;&#21033;&#23637;&#24320;&#21512;&#20316;&#65292;&#32780;&#30431;&#21451;&#26159;&#20234;&#25289;&#20811;&#24120;&#35268;&#20891;&#21644;&#24211;&#23572;&#24503;&#23433;&#20840;&#37096;&#38431;&#12290;&#33258;&#20174;&#22885;&#24052;&#39532;&#24635;&#32479;&#20960;&#21608;&#21069;&#25480;&#26435;&#31354;&#34989;&#20197;&#26469;&#65292;&#32654;&#20891;&#23601;&#19968;&#30452;&#22312;&#20026;&#36825;&#20108;&#32773;&#25552;&#20379;&#31354;&#20013;&#25903;&#25588;&#12290;</p>\n<div><img src=\"http://gedaodigest.qiniudn.com/wp-content/uploads/2014/09/IRAQ-2-articleLarge.jpg.jpeg\" alt=\"&#19978;&#21608;&#20116;&#65292;&#20234;&#25289;&#20811;&#37096;&#38431;&#21160;&#29992;&#20891;&#29992;&#30452;&#21319;&#26426;&#65292;&#20174;&#38463;&#33707;&#21033;&#25764;&#31163;&#22919;&#23418;&#12290;\" width=\"600\" height=\"400\"></p>\n<p>&#19978;&#21608;&#20116;&#65292;&#20234;&#25289;&#20811;&#37096;&#38431;&#21160;&#29992;&#20891;&#29992;&#30452;&#21319;&#26426;&#65292;&#20174;&#38463;&#33707;&#21033;&#25764;&#31163;&#22919;&#23418;&#12290;</p>\n</div>\n<p>&ldquo;&#21644;&#20160;&#21494;&#27966;&#27665;&#20853;&#20043;&#38388;&#30340;&#25152;&#26377;&#21327;&#35843;&#34892;&#21160;&#65292;&#37117;&#19981;&#26159;&#25105;&#20204;&#36827;&#34892;&#30340;&mdash;&mdash;&#32780;&#24212;&#35813;&#26159;&#30001;ISF&#36827;&#34892;&#30340;&#65292;&rdquo;&#22885;&#24052;&#39532;&#25919;&#24220;&#30340;&#19968;&#20301;&#39640;&#32423;&#23448;&#21592;&#21608;&#26085;&#34920;&#31034;&#65292;&#32780;&#27492;&#20154;&#21475;&#20013;&#30340;ISF&#25351;&#30340;&#26159;&#20234;&#25289;&#20811;&#23433;&#20840;&#37096;&#38431;(Iraqi Security Forces)&#12290;&#19981;&#36807;&#65292;&#20247;&#25152;&#21608;&#30693;&#30340;&#26159;&#65292;&#26368;&#36817;&#20960;&#20010;&#26376;&#65292;&#24403;&#26469;&#33258;ISIS&#30340;&#23041;&#32961;&#21464;&#24471;&#26126;&#26174;&#36215;&#26469;&#26102;&#65292;&#20160;&#21494;&#27966;&#27665;&#20853;&#32452;&#32455;&#19968;&#30452;&#22312;&#21644;ISF&#24182;&#32937;&#20316;&#25112;&#12290;</p>\n<p>&#21478;&#19968;&#21517;&#25919;&#24220;&#23448;&#21592;&#20063;&#21516;&#26102;&#34920;&#31034;&#65292;&#32654;&#22269;&#27809;&#26377;&#30452;&#25509;&#19982;&#20234;&#26391;&#21512;&#20316;&#12290;&ldquo;&#25105;&#20204;&#27491;&#22312;&#19982;&#20234;&#25289;&#20811;&#25919;&#24220;&#21644;&#20234;&#25289;&#20811;&#30340;&#24211;&#23572;&#24503;&#25954;&#27515;&#20891;&#21512;&#20316;&#65292;&rdquo;&#36825;&#21517;&#23448;&#21592;&#35828;&#12290;&ldquo;&#20165;&#27492;&#32780;&#24050;&#12290;&rdquo;</p>\n<p>&#24052;&#26684;&#36798;&#20197;&#21271;&#32422;105&#33521;&#37324;&#65288;&#32422;&#21512;170&#20844;&#37324;&#65289;&#30340;&#38463;&#33707;&#21033;&#20272;&#35745;&#26377;1.5&#19975;&#23621;&#27665;&#65292;&#22823;&#22810;&#37117;&#26159;&#34987;ISIS&#35270;&#20026;&#24322;&#25945;&#24466;&#30340;&#20160;&#21494;&#27966;&#22303;&#24211;&#26364;&#20154;&#12290;&#21608;&#26085;&#65292;&#23433;&#20840;&#23448;&#21592;&#34920;&#31034;&#65292;&#24182;&#27809;&#26377;&#23436;&#20840;&#25910;&#22797;&#35813;&#22320;&#21306;&#65292;&#20294;&#26159;&#65292;&#32852;&#20891;&#24050;&#32463;&#28165;&#38500;&#20102;&#20960;&#24231;&#26449;&#24196;&#30340;&#27494;&#35013;&#20998;&#23376;&#12290;</p>\n<p>&#21435;&#24180;&#65292;&#36225;&#30528;&#21465;&#21033;&#20122;&#20869;&#25112;&#20081;&#23616;&#24102;&#26469;&#30340;&#26426;&#20250;&#65292;ISIS&#22312;&#37027;&#37324;&#21344;&#25454;&#20102;&#22823;&#29255;&#22303;&#22320;&#65292;&#28982;&#21518;&#20316;&#20026;&#19968;&#32929;&#26356;&#24378;&#22823;&#30340;&#21183;&#21147;&#65292;&#22823;&#20030;&#36827;&#20891;&#23427;&#30340;&#35806;&#29983;&#22320;&#20234;&#25289;&#20811;&#65292;&#20196;&#20004;&#22269;&#20043;&#38388;&#30340;&#36793;&#30028;&#36880;&#28176;&#19981;&#22797;&#23384;&#22312;&#12290;ISIS&#22312;&#36825;&#20010;&#21160;&#33633;&#22320;&#21306;&#30340;&#36805;&#36895;&#22766;&#22823;&#65292;&#35753;&#20234;&#25289;&#20811;&#38754;&#20020;&#30528;&#20998;&#35010;&#30340;&#23041;&#32961;&#65292;&#20063;&#36843;&#20351;&#19968;&#30452;&#19981;&#24895;&#20171;&#20837;&#30340;&#22885;&#24052;&#39532;&#26356;&#21152;&#20840;&#38754;&#22320;&#20877;&#27425;&#25237;&#36523;&#20013;&#19996;&#20107;&#21153;&#12290;</p>\n<p>&#22312;&#20160;&#21494;&#27966;&#21344;&#32477;&#22823;&#22810;&#25968;&#30340;&#20234;&#26391;&#65292;ISIS&#30340;&#23835;&#36215;&mdash;&mdash;&#21450;&#20854;&#22312;&#35813;&#22320;&#21306;&#24314;&#31435;&#19968;&#20010;&#36874;&#23612;&#27966;&#21704;&#37324;&#21457;&#29579;&#22269;&#30340;&#20225;&#22270;&mdash;&mdash;&#24341;&#36215;&#20102;&#24503;&#40657;&#20848;&#30340;&#35686;&#24789;&#65292;&#22240;&#20026;&#36825;&#21487;&#33021;&#20250;&#23545;&#20234;&#26391;&#26412;&#36523;&#26500;&#25104;&#23041;&#32961;&#12290;&#20234;&#25289;&#20811;&#21644;&#21465;&#21033;&#20122;&#22343;&#19982;&#24503;&#40657;&#20848;&#20851;&#31995;&#23494;&#20999;&#65292;&#24182;&#19988;&#21161;&#20854;&#25193;&#22823;&#20102;&#22312;&#22320;&#21306;&#30340;&#21183;&#21147;&#33539;&#22260;&#65292;&#32780;ISIS&#27494;&#35013;&#20998;&#23376;&#30340;&#31361;&#28982;&#25104;&#21151;&#65292;&#26377;&#21487;&#33021;&#26356;&#20026;&#30452;&#25509;&#22320;&#21152;&#21095;&#36825;&#20004;&#20010;&#22269;&#23478;&#30340;&#21160;&#33633;&#12290;</p>\n<p>&#37492;&#20110;&#35813;&#22320;&#21306;&#26085;&#30410;&#38169;&#32508;&#22797;&#26434;&#30340;&#25919;&#27835;&#23616;&#38754;&#65292;&#22885;&#24052;&#39532;&#25919;&#24220;&#27491;&#22312;&#32771;&#34385;&#25171;&#20987;&#21465;&#21033;&#20122;&#22659;&#20869;&#30340;ISIS&#12290;</p>\n<p>&#22312;&#36825;&#20010;&#38382;&#39064;&#19978;&#65292;&#32654;&#22269;&#21644;&#20234;&#26391;&#30340;&#30446;&#26631;&#25130;&#28982;&#30456;&#21453;&#65306;&#20234;&#26391;&#26159;&#21465;&#21033;&#20122;&#24635;&#32479;&#24052;&#27801;&#23572;&middot;&#38463;&#33832;&#24503;(Bashar al-Assad)&#30340;&#37325;&#35201;&#25903;&#25345;&#32773;&#65292;&#32780;&#32654;&#22269;&#21017;&#24076;&#26395;&#36890;&#36807;&#25903;&#25345;&#28201;&#21644;&#30340;&#21453;&#23545;&#27966;&#25226;&#20182;&#36214;&#19979;&#21488;&#12290;&#20294;&#32654;&#22269;&#24320;&#23637;&#20219;&#20309;&#25171;&#20987;&#21465;&#21033;&#20122;&#22659;&#20869;&#30340;ISIS&#30340;&#20891;&#20107;&#34892;&#21160;&#65292;&#26368;&#32456;&#37117;&#21487;&#33021;&#20250;&#26377;&#21161;&#20110;&#24041;&#22266;&#38463;&#33832;&#24503;&#30340;&#22320;&#20301;&mdash;&mdash;&#24182;&#25512;&#36827;&#20234;&#26391;&#22312;&#35813;&#22320;&#21306;&#30340;&#35758;&#31243;&#12290;</p>\n<p>&#28041;&#21450;&#23447;&#27966;&#25112;&#20105;&#26102;&#65292;&#22885;&#24052;&#39532;&#25919;&#24220;&#19968;&#30452;&#35797;&#22270;&#36991;&#20813;&#26174;&#24471;&#22312;&#20559;&#34962;&#20854;&#20013;&#20219;&#20309;&#19968;&#26041;&#65292;&#22240;&#20026;&#20234;&#25289;&#20811;&#30340;&#36874;&#23612;&#27966;&#20154;&#22763;&#29305;&#21035;&#25285;&#24515;&#20160;&#21494;&#27966;&#27665;&#20853;&#32452;&#32455;&#12290;</p>\n<p>&#20294;&#26159;&#65292;&#33267;&#23569;&#22312;&#36825;&#19968;&#22825;&#37324;&#65292;&#20234;&#25289;&#20811;&#30340;&#29616;&#23454;&#29366;&#20917;&#65292;&#20284;&#20046;&#21387;&#20498;&#20102;&#23545;&#25903;&#25345;&#27665;&#20853;&#30340;&#20219;&#20309;&#25285;&#24515;&#12290;</p>\n<p>ISIS&#22312;&#20234;&#25289;&#20811;&#32902;&#34384;&#27178;&#34892;&#65292;&#23545;&#20440;&#34383;&#23454;&#26045;&#26025;&#39318;&#65292;&#23545;&#20160;&#21494;&#27966;&#36827;&#34892;&#23648;&#26432;&#65292;&#24182;&#39537;&#36880;&#20102;&#25968;&#21313;&#19975;&#23621;&#27665;&#12290;&#34429;&#28982;&#26377;&#20154;&#25351;&#36131;&#20160;&#21494;&#27966;&#27665;&#20853;&#36817;&#26399;&#25968;&#27425;&#34384;&#24453;&#36874;&#23612;&#27966;&#20154;&#22763;&#65292;&#20294;&#21040;&#30446;&#21069;&#20026;&#27490;&#65292;&#20182;&#20204;&#36824;&#27809;&#26377;&#24320;&#23637;&#36807;&#22823;&#35268;&#27169;&#20167;&#26432;&#12290;</p>\n<p>&#22312;&#20026;&#38463;&#33707;&#21033;&#32780;&#25112;&#30340;&#27665;&#20853;&#20013;&#65292;&#26377;&#34987;&#35748;&#20026;&#26159;&#20234;&#25289;&#20811;&#20160;&#21494;&#27966;&#27665;&#20853;&#32452;&#32455;&#20013;&#26368;&#20026;&#39559;&#20154;&#30340;&ldquo;&#27491;&#20041;&#32852;&#30431;&rdquo;(Asaib Ahl al-Haq)&#65292;&#36824;&#26377;&#19968;&#20010;&#32452;&#32455;&#21644;&#39047;&#20855;&#24433;&#21709;&#21147;&#30340;&#20160;&#21494;&#27966;&#31070;&#32844;&#20154;&#21592;&#31302;&#20811;&#22612;&#36798;&middot;&#33832;&#24503;&#23572;(Moktada al-Sadr)&#26377;&#20851;&#31995;&#12290;&#32654;&#20891;&#21344;&#39046;&#20234;&#25289;&#20811;&#26399;&#38388;&#65292;&#27492;&#20154;&#26159;&#32654;&#22269;&#20154;&#26368;&#24378;&#30828;&#30340;&#25932;&#20154;&#20043;&#19968;&#12290;&#36825;&#20123;&#22242;&#20307;&#37117;&#33719;&#24471;&#20102;&#20234;&#26391;&#30340;&#25903;&#25345;&#12290;</p>\n<p>&#32654;&#22269;&#36880;&#27493;&#32553;&#20943;&#22312;&#20234;&#25289;&#20811;&#30340;&#20891;&#20107;&#20316;&#29992;&#26399;&#38388;&#65292;&ldquo;&#27491;&#20041;&#32852;&#30431;&rdquo;&#26159;&#19968;&#20010;&#2930