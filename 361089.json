&#33021;&#25163;&#34920;&#65292;&#21487;&#21516;&#26102;&#20316;&#20026;&#33021;&#36861;&#36394;&#27493;&#25968;&#12289;&#21345;&#36335;&#37324;&#12289;&#24515;&#29575;&#31561;&#20449;&#24687;&#30340;&#20581;&#36523;&#36741;&#21161;&#35774;&#22791;&#65307;&#20854;&#26032;&#31227;&#21160;&#25805;&#20316;&#31995;&#32479;iOS 8&#23558;&#23884;&#20837;&#21517;&#20026;Health&#30340;&#24212;&#29992;&#65292;&#36825;&#26159;&#19968;&#20010;&#21487;&#20197;&#26174;&#31034;&#24515;&#29575;&#12289;&#21345;&#36335;&#37324;&#28040;&#32791;&#12289;&#34880;&#31958;&#21644;&#32966;&#22266;&#37255;&#31561;&#20581;&#24247;&#21644;&#20581;&#36523;&#20449;&#24687;&#20197;&#21450;&#21270;&#39564;&#32467;&#26524;&#21644;&#29992;&#33647;&#30340;&#20202;&#34920;&#30424;&#65307;&#36824;&#26377;HealthKit&mdash;&mdash;&#33529;&#26524;&#26368;&#36817;&#25581;&#24149;&#30340;&#20581;&#24247;&#21644;&#20581;&#36523;&#25968;&#25454;&#23384;&#20648;&#24211;&#65292;&#21487;&#20197;&#19982;&#21307;&#29983;&#21644;&#20854;&#20182;&#21307;&#30103;&#25552;&#20379;&#21830;&#36830;&#25509;&#12290;</p>\n<p>&#20581;&#24247;&#25968;&#25454;&#24456;&#25935;&#24863;&#65292;&#32780;&#19988;&#33529;&#26524;&#23545;&#36825;&#20123;&#25968;&#25454;&#23558;&#34987;&#22914;&#20309;&#20351;&#29992;&#21644;&#20445;&#25252;&#24182;&#26410;&#35848;&#24471;&#22826;&#22810;&#12290;&#33529;&#26524;&#21608;&#20108;&#26410;&#22238;&#35206;&#23547;&#27714;&#32622;&#35780;&#30340;&#35831;&#27714;&#12290;&#20197;&#19979;&#26159;&#19968;&#20123;&#38382;&#39064;&#65306;</p>\n<p>&#33529;&#26524;&#23558;&#22914;&#20309;&#22788;&#29702;&#23558;&#25968;&#25454;&#21334;&#32473;&#24191;&#21578;&#20027;&#21644;&#20854;&#20182;&#31532;&#19977;&#26041;&#30340;&#24212;&#29992;&#21644;&#35774;&#22791;&#65311;</p>\n<p>&#20170;&#26085;&#24066;&#22330;&#20013;&#30340;&#32422;4&#19975;&#20010;&#20581;&#24247;&#24212;&#29992;&#21644;&#21487;&#31359;&#25140;&#35774;&#22791;&#37324;&#65292;&#24456;&#22810;&#37117;&#36890;&#36807;&#21521;&#21334;&#23478;&#21644;&#20854;&#20182;&#20844;&#21496;&#20986;&#21806;&#29992;&#25143;&#25968;&#25454;&#36186;&#38065;&#12290;</p>\n<p>&#33529;&#26524;&#36817;&#26085;&#23545;&#24320;&#21457;&#20154;&#21592;&#35828;&#65292;&#24212;&#29992;&#31243;&#24207;&#19981;&#33021;&#23558;&#20854;&#36890;&#36807;HealthKit&#39033;&#30446;&#25910;&#38598;&#21040;&#30340;&#29992;&#25143;&#25968;&#25454;&#29992;&#20110;&#24191;&#21578;&#21644;&#38500;&ldquo;&#25913;&#36827;&#20581;&#24247;&#12289;&#21307;&#30103;&#21644;&#20581;&#36523;&#31649;&#29702;&#20197;&#21450;&#21307;&#30103;&#30740;&#31350;&#20043;&#30446;&#30340;&rdquo;&#20043;&#22806;&#30340;&#20854;&#20182;&#25968;&#25454;&#25366;&#25496;&#30340;&#30446;&#30340;&#12290;&#38656;&#35201;&#21033;&#29992;HealthKit&#25968;&#25454;&#30340;&#24212;&#29992;&#31243;&#24207;&#37117;&#35201;&#27714;&#26377;&#38544;&#31169;&#25919;&#31574;&#12290;</p>\n<p>&#20294;&#19968;&#20123;&#38544;&#31169;&#38382;&#39064;&#19987;&#23478;&#35828;&#65292;&#36825;&#20854;&#20013;&#20173;&#23384;&#22312;&#35299;&#35835;&#31354;&#38388;&#12290;&#38750;&#33829;&#21033;&#32452;&#32455;Patient Privacy Rights Foundation&#30340;&#21019;&#22987;&#20154;&#30382;&#23572;(Deborah Peel)&#25351;&#20986;&#65292;&#33529;&#26524;&#30340;&#25351;&#23548;&#21407;&#21017;&#24182;&#27809;&#26377;&#20855;&#20307;&#35201;&#27714;&#24212;&#29992;&#31243;&#24207;&#20844;&#21496;&#20026;&#20854;&#20219;&#20309;&#25259;&#38706;&#36127;&#36131;&#12290;&#22905;&#35828;&#65292;&#29992;&#25143;&#24212;&#35813;&#35201;&#30830;&#20999;&#22320;&#30475;&#21040;&#65292;&#36825;&#20123;&#24212;&#29992;&#31243;&#24207;&#26159;&#22914;&#20309;&#20351;&#29992;&#20182;&#20204;&#30340;&#25968;&#25454;&#30340;&#12290;</p>\n<p>&#38544;&#31169;&#26435;&#20027;&#24352;&#22242;&#20307;Privacy Rights Clearinghouse&#26368;&#36817;&#23545;&#19968;&#20123;&#24212;&#29992;&#36827;&#34892;&#20102;&#26816;&#27979;&#65292;&#20854;&#20013;&#19977;&#20998;&#20043;&#19968;&#21521;&#31532;&#19977;&#26041;&#21457;&#36865;&#25968;&#25454;&#65292;&#20294;&#24212;&#29992;&#24320;&#21457;&#32773;&#24182;&#27809;&#26377;&#23545;&#27492;&#36827;&#34892;&#25259;&#38706;&#12290;&#36825;&#21487;&#33021;&#20196;&#33529;&#26524;&#24378;&#21046;&#35201;&#27714;&#25191;&#34892;&#30456;&#20851;&#35268;&#23450;&#21464;&#24471;&#22256;&#38590;&#12290;</p>\n<p>&#20581;&#24247;&#38544;&#31169;&#19987;&#23478;&#12289;Manatt, Phelps &amp; Phillips LLP&#24459;&#24072;&#20107;&#21153;&#25152;&#36127;&#36131;&#21307;&#30103;&#23454;&#36341;&#26696;&#20214;&#30340;&#21512;&#20249;&#20154;&#40614;&#26684;&#21171;(Deven McGraw)&#35828;&#65292;&#21478;&#19968;&#26041;&#38754;&#65292;&#22914;&#26524;&#33529;&#26524;&#22312;&#30830;&#20445;&#24212;&#29992;&#31243;&#24207;&#36981;&#23432;&#38544;&#31169;&#35268;&#23450;&#26041;&#38754;&#20570;&#24471;&#24456;&#22909;&#65292;&#32654;&#22269;&#20154;&#21487;&#33021;&#20250;&#24471;&#21040;&#27604;&#24403;&#21069;&#26356;&#22909;&#30340;&#20445;&#25252;&#12290;</p>\n<p>&#38544;&#31169;&#26435;&#20027;&#24352;&#22242;&#20307;Electronic Privacy Information Center&#30340;&#24635;&#35009;&#32599;&#28373;&#36125;&#26684;(Marc Rotenberg)&#35828;&#65292;&#30456;&#20851;&#38382;&#39064;&#24050;&#32463;&#36229;&#36234;&#20102;&#24191;&#21578;&#21830;&#23618;&#38754;&#12290;&#20030;&#20363;&#26469;&#35828;&#65292;&#22914;&#26524;DNA&#20449;&#24687;&#34987;&#32622;&#20837;&#26576;&#19968;&#24212;&#29992;&#65292;&#37027;&#20040;&#25191;&#27861;&#37096;&#38376;&#25110;&#32773;&#27861;&#23448;&#23601;&#26377;&#21487;&#33021;&#22312;&#31163;&#23130;&#25110;&#30417;&#31649;&#26435;&#35785;&#35772;&#20013;&#35851;&#27714;&#20351;&#29992;&#36825;&#20123;&#20449;&#24687;&#12290;&#20174;&#20107;&#23398;&#26415;&#30740;&#31350;&#30340;&#20154;&#22763;&#21487;&#33021;&#20250;&#24076;&#26395;&#20351;&#29992;&#27492;&#31867;&#25968;&#25454;&#12290;</p>\n<p>&#32599;&#28373;&#36125;&#26684;&#31216;&#65292;&#33529;&#26524;&#23578;&#26410;&#34920;&#31034;&#23558;&#22914;&#20309;&#22788;&#29702;&#27492;&#31867;&#24773;&#20917;&#12290;&#20182;&#34920;&#31034;&#65292;&#38543;&#30528;&#33529;&#26524;&#24320;&#23637;&#25910;&#38598;&#29992;&#25143;&#25968;&#25454;&#30340;&#19994;&#21153;&#65292;&#35813;&#20844;&#21496;&#23558;&#36935;&#21040;&#26356;&#22810;&#38544;&#31169;&#38382;&#39064;&#12290;</p>\n<p>&#33529;&#26524;&#26159;&#21542;&#20250;&#23581;&#35797;&#21033;&#29992;&#20154;&#20204;&#21521;&#36825;&#20123;&#26032;&#24179;&#21488;&#19978;&#20256;&#30340;&#25968;&#25454;&#36186;&#38065;&#65311;</p>\n<p>&#33529;&#26524;&#24050;&#19982;&#30005;&#23376;&#30149;&#21382;&#25552;&#20379;&#21830;Epic&#24314;&#31435;&#21512;&#20316;&#20851;&#31995;&#65292;&#21521;HealthKit&#23548;&#20837;&#25968;&#25454;&#65292;&#19981;&#36807;&#30456;&#20851;&#32454;&#33410;&#23578;&#26410;&#25259;&#38706;&#12290;&#33529;&#26524;&#22312;&#22788;&#29702;&#38544;&#31169;&#26041;&#38754;&#26377;&#33391;&#22909;&#30340;&#35760;&#24405;&#65292;&#20294;&#23545;&#20110;HealthKit&#30340;&#19994;&#21153;&#27169;&#24335;&#20132;&#24453;&#24471;&#19981;&#22810;&#12290;</p>\n<p>HealthKit&#30340;&#20986;&#29616;&#23558;&#25512;&#21160;&#21307;&#30103;&#20445;&#20581;&#26381;&#21153;&#25552;&#20379;&#21830;&#20026;&#30149;&#20154;&#24320;&#21457;&#19968;&#20123;&#24037;&#20855;&#65292;&#36825;&#20123;&#24037;&#20855;&#21487;&#20197;&#25910;&#38598;&#20010;&#20154;&#20581;&#24247;&#25968;&#25454;&#65288;&#20363;&#22914;&#20986;&#38498;&#21518;&#26159;&#21542;&#36827;&#34892;&#38203;&#28860;&#65289;&#20197;&#25552;&#20379;&#32473;&#21307;&#38498;&#25110;&#21307;&#29983;&#12290;</p>\n<p>&#21307;&#23398;&#30740;&#31350;&#20154;&#21592;&#21644;&#20445;&#38505;&#20844;&#21496;&#28212;&#26395;&#33719;&#24471;&#27492;&#31867;&#25968;&#25454;&#12290;&#20363;&#22914;&#65292;&#22914;&#26524;&#25968;&#20197;&#30334;&#19975;&#35745;&#30340;&#30149;&#20154;&#23558;&#33258;&#24049;&#30340;&#21307;&#30103;&#35760;&#24405;&#19978;&#20256;&#33267;HealthKit&#65292;&#21307;&#38498;&#21644;&#20445;&#38505;&#20844;&#21496;&#21487;&#33021;&#20250;&#26377;&#20852;&#36259;&#20998;&#26512;&#30446;&#26631;&#20154;&#32676;&#30340;&#20581;&#24247;&#25351;&#26631;&#65288;&#20363;&#22914;&#27493;&#34892;&#25968;&#12289;&#24515;&#29575;&#25110;&#33647;&#29289;&#27835;&#30103;&#26041;&#26696;&#65289;&#12290;</p>\n<p>&#33529;&#26524;&#20250;&#20986;&#21806;&#36825;&#20123;&#29992;&#25143;&#25968;&#25454;&#21527;&#65311;&#22914;&#26524;&#26159;&#30340;&#35805;&#65292;&#33529;&#26524;&#23558;&#20986;&#21806;&#20010;&#20154;&#25968;&#25454;&#36824;&#26159;&#25171;&#21253;&#20986;&#21806;&#25968;&#25454;&#65311;&#29992;&#25143;&#21448;&#23558;&#34987;&#35201;&#27714;&#25552;&#20379;&#20160;&#20040;&#26679;&#30340;&#35768;&#21487;&#21602;&#65311;</p>\n<p>&#33529;&#26524;&#23558;&#22914;&#20309;&#20197;&#21450;&#22312;&#20309;&#31181;&#29615;&#22659;&#19979;&#36981;&#23432;&#20581;&#24247;&#38544;&#31169;&#27861;&#65311;</p>\n<p>&#22312;&#32654;&#22269;&#65292;&#27719;&#32534;&#24182;&#23384;&#20648;&#30149;&#24739;&#21307;&#30103;&#35760;&#24405;&#30340;&#20445;&#38505;&#21830;&#21644;&#21307;&#30103;&#26381;&#21153;&#20379;&#24212;&#21830;&#24517;&#39035;&#36981;&#24490;&#12298;&#20581;&#24247;&#20445;&#38505;&#20415;&#25658;&#21644;&#36131;&#20219;&#27861;&#26696;&#12299;(Health Insurance Portability and Accountability Act, &#31616;&#31216;HIPAA)&#12290;&#26681;&#25454;&#35813;&#27861;&#26696;&#65292;&#23384;&#20648;&#36825;&#31867;&#25968;&#25454;&#30340;&#23454;&#20307;&#19981;&#24471;&#22312;&#26410;&#32463;&#24739;&#32773;&#20801;&#35768;&#30340;&#24773;&#20917;&#19979;&#20197;&#33829;&#38144;&#25110;&#30740;&#31350;&#20026;&#30446;&#30340;&#20351;&#29992;&#25110;&#20986;&#21806;&#24739;&#32773;&#30340;&#21307;&#30103;&#35760;&#24405;&#12290;&#35813;&#27861;&#26696;&#36824;&#35268;&#23450;&#65292;&#19968;&#26086;&#21457;&#29983;&#25968;&#25454;&#27844;&#28431;&#65292;&#36825;&#31867;&#23454;&#20307;&#24517;&#39035;&#21521;&#25919;&#24220;&#25253;&#21578;&#65292;&#20844;&#21496;&#36824;&#24517;&#39035;&#19982;&#29992;&#25143;&#36798;&#25104;&#38544;&#31169;&#21644;&#23433;&#20840;&#21327;&#35758;&#12290;</p>\n<p>&#32852;&#37030;&#25919;&#24220;&#23448;&#21592;&#27492;&#21069;&#34920;&#31034;&#65292;&#19978;&#20256;&#21040;&#27492;&#31867;&#24212;&#29992;&#30340;&#29992;&#25143;&#25968;&#25454;&#22823;&#22810;&#19981;&#22312;HIPAA&#20445;&#25252;&#33539;&#22260;&#20869;&#65292;&#20294;HealthKit&#21487;&#20197;&#34987;&#35774;&#32622;&#20026;&#25910;&#21462;&#21463;HIPAA&#20445;&#25252;&#30340;&#19968;&#20123;&#25968;&#25454;&#65292;&#20363;&#22914;&#21307;&#30103;&#35760;&#24405;&#12289;&#21270;&#39564;&#32467;&#26524;&#31561;&#12290;</p>\n<p>&#33529;&#26524;&#27809;&#26377;&#35828;HIPAA&#20445;&#25252;&#26465;&#27454;&#26159;&#21542;&#23558;&#36866;&#29992;&#20110;&#20844;&#21496;&#25910;&#38598;&#21040;&#30340;&#36825;&#31867;&#25935;&#24863;&#25968;&#25454;&#12290;&#36825;&#24847;&#21619;&#30528;&#65292;&#33529;&#26524;&#19981;&#19968;&#23450;&#38656;&#35201;&#25253;&#21578;&#31867;&#20284;&#36817;&#26399;iCloud&#36134;&#25143;&#34987;&#40657;&#36825;&#26679;&#30340;&#25968;&#25454;&#27844;&#28431;&#38382;&#39064;&#12290;&#22312;&#26368;&#36817;&#36825;&#36215;&#20107;&#20214;&#20013;&#65292;&#40657;&#23458;&#20837;&#20405;&#22810;&#20301;&#26126;&#26143;&#30340;iCloud&#36134;&#25143;&#12289;&#31363;&#21462;&#20102;&#35064;&#29031;&#65292;&#24182;&#25226;&#29031;&#29255;&#21457;&#24067;&#21040;&#20114;&#32852;&#32593;&#19978;&#12290;</p>\n<p>&#33529;&#26524;&#26368;&#36817;&#34920;&#31034;&#65292;&#19981;&#20250;&#22312;iCloud&#22791;&#20221;&#20581;&#24247;&#25968;&#25454;&#65292;&#32780;&#19988;&#20581;&#24247;&#25968;&#25454;&#23558;&#34987;&#21152;&#23494;&#12290;</p>\n<p>&#26410;&#26469;&#28040;&#36153;&#32773;&#20250;&#20381;&#36182;&#20581;&#24247;&#36719;&#20214;&#32780;&#19981;&#21435;&#30475;&#21307;&#29983;&#21527;&#65311;</p>\n<p>&#20581;&#24247;&#25968;&#25454;&#19987;&#23478;&#27492;&#21069;&#23601;&#24050;&#25351;